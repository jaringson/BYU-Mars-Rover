<launch>

  <node name="gate_detector" pkg="gate_detector" type="gate_detector.py">
    <param name="_hue_lower" value="35" />
    <param name="_sat_lower" value="147" />
    <param name="_val_lower" value="98" />
    <param name="_hue_upper" value="56" />
    <param name="_sat_upper" value="255" />
    <param name="_val_upper" value="255" />


  </node>
  
  <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
    <param name="video_device" value="/dev/video1" />
    <param name="image_width" value="640" />
    <param name="image_height" value="480" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_frame_id" value="usb_cam" />
    <param name="io_method" value="mmap"/>
    <remap from="image_raw" to="/usb_cam/image_raw" />
    
  </node>

  </launch>