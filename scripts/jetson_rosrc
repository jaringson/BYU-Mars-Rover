# To Run ROS Locally (No Network)
#source ~/git/BYU-Mars-Rover/scripts/set_ros_master localhost

# To Set Rover as Master for Network
source ~/git/BYU-Mars-Rover/scripts/set_ros_master 192.168.1.69
export ROS_HOSTNAME=192.168.1.69
export ROS_MASTER_URI=http://192.168.1.69:11311
export ROS_IP=192.168.1.69

export ROSLAUNCH_SSH_UNKNOWN=1

# ROS WORKSPACE
source /opt/ros/kinetic/setup.bash

source ~/git/BYU-Mars-Rover/onboard_ws/devel/setup.bash

